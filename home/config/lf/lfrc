cmd open ${{
	case $(file --mime-type -Lb "$f") in
 		text/*) 
		    nvim $fx
		    ;;
		image/*)
		    imv "$fx" > /dev/null 2>&1 &
		    ;;
		application/pdf)
		    zathura "$fx" > /dev/null 2>&1 &
		    ;;
		application/gzip)
		    xournalpp "$fx" > /dev/null 2>&1 &
		    ;;
		*) 
		    for f in $fx; do
			echo $f $(file --mime-type -Lb $f)| less;
			# xdg-open $f > /dev/null 2> /dev/null &
		    done
		    ;;
	esac
}}
